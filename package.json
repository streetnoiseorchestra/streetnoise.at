{
  "name": "streetnoise-at",
  "version": "0.0.2",
  "description": "",
  "author": "Casey Link",
  "license": "GPLv3+",
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/helper-module-imports": "^7.10.1",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "autoprefixer": "^9.8.0",
    "bulma": "^0.8.2",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.12.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "scripts": {
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "npm run css-build-sno && npm run css-build-strafiato",
    "css-build-sno": "node-sass _css/streetnoise.scss streetnoise/static/css/streetnoise.css",
    "css-build-strafiato": "node-sass _css/strafiato.scss streetnoise/static/css/strafiato.css",
    "css-postcss": "postcss --use autoprefixer --output streetnoise/static/css/streetnoise.css streetnoise/static/css/streetnoise.css",
    "css-watch": "npm run css-build-sno -- --watch",
    "deploy": "npm run css-deploy && npm run js-build",
    "js-build": "npx babel _javascript --out-dir streetnoise/static/js",
    "js-watch": "npm run js-build -- --watch",
    "wagtail": "make serve",
    "start": "npm-run-all --parallel css-watch js-watch wagtail"
  },
  "dependencies": {
    "bulma-accordion": "^2.0.1",
    "bulma-extensions": "^6.2.7"
  }
}
